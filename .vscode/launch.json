{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Lancer",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/server/app.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/server",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--use_strict",
				"--harmony"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "internalConsole",
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Attacher",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"sourceMaps": false,
			"outDir": null,
			"localRoot": "${workspaceRoot}/server",
			"remoteRoot": null
		},
		{
			"name": "Attacher au processus",
			"type": "node",
			"request": "attach",
			"processId": "${command.PickProcess}",
			"port": 5858,
			"sourceMaps": false,
			"outDir": null
		}
	]
}